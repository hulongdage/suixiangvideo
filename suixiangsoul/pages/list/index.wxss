.container {
  background-color: #F2f2f2;
}
view.text {
  display: flex;
  align-items: center;
  position: relative;
  padding: 10px 10px 10px 1%;
  font-size: 15px;
  color: #656565;
}

view.text:before {
    position: absolute;
    display: block;
    content: ' ';
    left: -5px;
    width: 2px;
    height: 100%;
    background-color: #FF8601;
}
.line_y {
  width: 3px;
  height:18px;
  display: inline-block;
  background-color: #FF8601;
}
view.text text {
  margin-left: 10px;
  line-height: 18px;
}
